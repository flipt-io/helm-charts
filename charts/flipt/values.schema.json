      "additionalProperties": true,
      "properties": {
        "serviceMonitor": {
          "additionalProperties": true,
          "properties": {
            "annotations": {
              "additionalProperties": true,
              "description": "ServiceMonitor annotations",
              "title": "annotations",
              "type": "object",
              "default": {}
            },
            "enabled": {
              "default": false,
              "description": "If enabled, ServiceMonitor resources for Prometheus Operator are created",
              "title": "enabled",
              "type": "boolean"
            },
            "endpoint": {
              "additionalProperties": true,
              "title": "endpoint",
              "type": "object"
            },
            "honorLabels": {
              "default": false,
              "description": "What to do when flipt metrics labels are conflicting with Prometheus server-side labels.\ntrue means keep label values from flipt.",
              "title": "honorLabels",
              "type": "boolean"
            },
            "interval": {
              "default": "15s",
              "description": "ServiceMonitor scrape interval\nDefault is 15s because included recording rules use a 1m rate, and scrape interval needs to be at\nleast 1/4 rate interval.",
              "title": "interval",
              "type": "string"
            },
            "labels": {
              "additionalProperties": true,
              "description": "Additional ServiceMonitor labels",
              "title": "labels",
              "type": "object",
              "default": {}
            },
            "namespaceSelector": {
              "additionalProperties": true,
              "description": "Namespace selector for ServiceMonitor resources",
              "title": "namespaceSelector",
              "type": "object",
              "default": {}
            },
            "port": {
              "default": "http",
              "description": "Port where to scrape metrics from",
              "title": "port",
              "type": "string"
            },
            "relabelings": {
              "description": "ServiceMonitor relabel configs to apply to samples before scraping. See: https://github.com/prometheus-operator/prometheus-operator/blob/master/Documentation/api.md#relabelconfig",
              "items": {},
              "title": "relabelings",
              "type": "array",
              "default": []
            },
            "scheme": {
              "default": "http",
              "description": "ServiceMonitor will use http by default, but you can pick https as well",
              "title": "scheme",
              "type": "string"
            },
            "scrapeTimeout": {
              "description": "ServiceMonitor scrape timeout in Go duration format (e.g. 15s)",
              "title": "scrapeTimeout",
              "type": ["string", "null"]
            },
            "sampleLimit": {
              "title": "sampleLimit",
              "type": "integer"
            },
            "targetLabels": {
              "items": {
                "type": "string"
              },
              "title": "targetLabels",
              "type": "array"
            },
            "tlsConfig": {
              "additionalProperties": true,
              "title": "tlsConfig",
              "type": ["object", "null"]
            }
          },
          "title": "serviceMonitor",
          "type": "object",
          "default": {
            "enabled": false,
            "port": "http",
            "namespaceSelector": {},
            "annotations": {},
            "labels": {},
            "interval": "15s",
            "relabelings": [],
            "honorLabels": false,
            "scheme": "http"
          }
        }
      },
      "title": "metrics",
      "type": "object"
